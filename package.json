{
  "name": "elephicon",
  "productName": "Elephicon",
  "version": "0.7.2",
  "description": "Elephicon generates Apple ICNS and Microsoft ICO files from PNG files.",
  "author": "sprout2000",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:sprout2000/elephicon.git"
  },
  "bugs": {
    "url": "https://github.com/sprout2000/elephicon/issues"
  },
  "keywords": [
    "electron",
    "ico",
    "icns",
    "png2icons"
  ],
  "engines": {
    "node": ">=10.14",
    "npm": "please_use_yarn_instead",
    "yarn": ">=1.22"
  },
  "os": [
    "win32",
    "darwin"
  ],
  "main": "dist/main.js",
  "scripts": {
    "start": "run-s clean build serve",
    "serve": "electron .",
    "clean": "rimraf dist release",
    "build": "cross-env NODE_ENV=\"development\" webpack --progress",
    "build:prod": "cross-env NODE_ENV=\"production\" webpack --progress",
    "release": "node scripts/build.js",
    "deploy": "gh-pages -d public",
    "prepackage": "run-s clean build:prod",
    "package": "cross-env CSC_IDENTITY_AUTO_DISCOVERY=false node scripts/build-unsign.js",
    "prepackage:appx": "run-s clean build:prod",
    "package:appx": "node scripts/build.js",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:push": "git push --follow-tags origin master"
  },
  "dependencies": {
    "electron-log": "^4.2.2",
    "electron-store": "^5.2.0",
    "electron-updater": "^4.3.4",
    "mime-types": "^2.1.27",
    "png2icons": "^2.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "typeface-roboto": "^0.0.75",
    "ua-parser-js": "^0.7.21"
  },
  "devDependencies": {
    "@types/electron-load-devtool": "^1.2.0",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "@types/ua-parser-js": "^0.7.33",
    "@types/webpack": "^4.41.21",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.0",
    "dotenv": "^8.2.0",
    "electron": "^8.4.0",
    "electron-builder": "^22.8.0",
    "electron-load-devtool": "^1.2.0",
    "electron-notarize": "^1.0.0",
    "html-webpack-plugin": "^4.3.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "rimraf": "^3.0.2",
    "sass-loader": "^9.0.2",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^4.0.0",
    "ts-loader": "^8.0.2",
    "typescript": "^3.9.7",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "optionalDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "gh-pages": "^3.1.0",
    "prettier": "^2.0.5",
    "standard-version": "^8.0.2"
  }
}
