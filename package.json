{
  "name": "gen-icns",
  "productName": "GenICNS",
  "version": "0.1.1",
  "description": "GenICNS generates Apple ICNS and Microsoft ICO files from PNG files.",
  "author": "sprout2000",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:sprout2000/gen-icns.git"
  },
  "bugs": {
    "url": "https://github.com/sprout2000/gen-icns/issues"
  },
  "keywords": [
    "electron",
    "ico",
    "icns",
    "png2icons"
  ],
  "engines": {
    "node": ">=10"
  },
  "main": "dist/main.js",
  "scripts": {
    "start": "run-s clean build serve",
    "serve": "electron .",
    "clean": "rimraf dist release",
    "build": "cross-env NODE_ENV=\"development\" webpack --progress",
    "build:prod": "cross-env NODE_ENV=\"production\" webpack --progress",
    "release": "node scripts/build.js",
    "prepackage": "run-s clean build:prod",
    "package": "node scripts/build-unsign.js",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:push": "git push --follow-tags origin master"
  },
  "dependencies": {
    "electron-log": "^4.1.2",
    "electron-store": "^5.1.1",
    "electron-updater": "^4.3.1",
    "mime-types": "^2.1.27",
    "png2icons": "^2.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-switch": "^5.0.1",
    "typeface-roboto": "0.0.75"
  },
  "devDependencies": {
    "@types/electron-load-devtool": "^1.2.0",
    "@types/mime-types": "^2.1.0",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/webpack": "^4.41.12",
    "@typescript-eslint/eslint-plugin": "^2.32.0",
    "@typescript-eslint/parser": "^2.32.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "dotenv": "^8.2.0",
    "electron": "^8.2.5",
    "electron-builder": "^22.6.0",
    "electron-load-devtool": "^1.2.0",
    "electron-notarize": "^0.3.0",
    "eslint": "^6",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "html-webpack-plugin": "^4.3.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "standard-version": "^8.0.0",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.4",
    "typescript": "^3.8.3",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
